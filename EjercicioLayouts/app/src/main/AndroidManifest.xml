<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.institutoserpis.juanminm.ejerciciolayouts">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.institutoserpis.juanminm.ejerciciolayouts.LinearLayoutActivity"
            android:parentActivityName="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity" />
        </activity>
        <activity android:name="org.institutoserpis.juanminm.ejerciciolayouts.TableLayoutActivity"
            android:parentActivityName="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity" />
        </activity>
        <activity android:name="org.institutoserpis.juanminm.ejerciciolayouts.RelativeLayoutActivity"
            android:parentActivityName="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity" />
        </activity>
        <activity android:name="org.institutoserpis.juanminm.ejerciciolayouts.GridLayoutActivity"
            android:parentActivityName="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="org.institutoserpis.juanminm.ejerciciolayouts.MainActivity" />
        </activity>
    </application>

</manifest>